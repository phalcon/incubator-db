{
  "name": "phalcon/incubator-db",
  "description": "Phalcon Incubator DB",
  "keywords": [
    "framework",
    "phalcon",
    "incubator",
    "database",
    "adapters"
  ],
  "homepage": "https://phalcon.io",
  "license": "BSD-3-Clause",
  "authors": [{
      "name": "Phalcon Team",
      "email": "team@phalcon.io",
      "homepage": "https://phalcon.io/en/team"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/phalcon/incubator-db/graphs/contributors"
    }
  ],
  "support": {
    "issues": "https://github.com/phalcon/incubator-db/issues",
    "source": "https://github.com/phalcon/incubator-db",
    "forum": "https://forum.phalcon.io"
  },
  "require": {
    "php": ">=7.4",
    "ext-phalcon": "5.0.0beta2"
  },
  "require-dev": {
    "phalcon/ide-stubs": "^4.1",
    "vimeo/psalm": "^4.7",
    "squizlabs/php_codesniffer": "^3.5",
    "codeception/codeception": "^4.1",
    "codeception/module-asserts": "^1.0.0",
    "phpstan/phpstan": "^0.12.18"
  },
  "autoload": {
    "psr-4": {
      "Phalcon\\Incubator\\Db\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phalcon\\Incubator\\Db\\Tests\\Unit\\": "tests/unit/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "archive": {
    "exclude": [
      "/.github",
      "/tests",
      ".codacy.yml",
      ".codecov.yml",
      "CHANGELOG.md",
      "codeception.yml",
      "phpcs.xml.dist",
      "phpstan.neon.dist",
      "phpunit.xml.dist",
      "psalm.xml.dist"
    ]
  },
  "config": {
    "platform": {
      "php": "7.4"
    }
  }
}